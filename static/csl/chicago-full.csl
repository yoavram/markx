<style xmlns="http://purl.org/net/xbiblio/csl" class="note" demote-non-dropping-particle="sort-only" page-range-format="chicago" version="1.1mlz1" year-range-format="expanded">
  <info>
    <title>MLZ Chicago Manual of Style (Full Note)</title>
    <id>http://citationstyles.org/styles/mlz-cms-full</id>
    <link href="http://citationstyles.org/styles/mlz-cms-full" rel="self"/>
    <link href="http://www.chicagomanualofstyle.org/tools_citationguide.html" rel="documentation"/>
    <author>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </author>
    <contributor>
      <name>Simon Kornblith</name>
      <email>simon@simonster.com</email>
    </contributor>
    <contributor>
      <name>Elena Razlogova</name>
      <email>elena.razlogova@gmail.com</email>
    </contributor>
    <contributor>
      <name>Frank Bennett</name>
      <email>biercenator@gmail.com</email>
    </contributor>
    <summary>Chicago format with full notes and bibliography (MLZ version)</summary>
    <category field="generic-base"/>
    <category citation-format="note"/>
    <updated>2012-10-03T11:52:18.875650</updated>
    <rights>This work is licensed under a Creative Commons Attribution-Share Alike 3.0 License: http://creativecommons.org/licenses/by-sa/3.0/</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="article">article</term>
      <term name="director">director</term>
      <term form="verb" name="director">directed by</term>
      <term form="verb-short" name="director">dir.</term>
      <term form="short" name="director">dir.</term>
      <term form="long" name="article">
        <single>article</single>
        <multiple>articles</multiple>
      </term>
      <term form="short" name="article">
        <single>art.</single>
        <multiple>arts.</multiple>
      </term>
      <term form="long" name="Section">
        <single>section</single>
        <multiple>sections</multiple>
      </term>
      <term form="short" name="Section">
        <single>sec.</single>
        <multiple>secs.</multiple>
      </term>
      <term form="long" name="Chapter">
        <single>chapter</single>
        <multiple>chapters</multiple>
      </term>
      <term form="short" name="Chapter">
        <single>ch.</single>
        <multiple>chs.</multiple>
      </term>
      <term form="symbol" name="Chapter">
        <single>c.</single>
        <multiple>cc.</multiple>
      </term>
      <term name="title">title</term>
      <term form="short" name="title">tit.</term>
      <term form="short" name="section">sec.</term>
      <term form="symbol" name="section">
        <single>&#167;</single>
        <multiple>&#167;&#167;</multiple>
      </term>
      <term form="verb-short" name="editortranslator">
        <single>trans. and ed.</single>
        <multiple>trans. and ed.</multiple>
      </term>
      <term form="verb" name="editortranslator">
        <single> Translated and edited by</single>
        <multiple> Translated and edited by</multiple>
      </term>
    </terms>
  </locale>
  <macro name="std-authority-key">
    <names variable="authority">
      <name/>
      <institution form="short"/>
    </names>
  </macro>
  <macro name="std-authority-full">
    <group delimiter=" ">
      <choose>
        <if subjurisdictions="2">
          <text macro="std-authority-child"/>
          <text form="short" variable="jurisdiction"/>
        </if>
        <else>
          <text form="short" variable="jurisdiction"/>
          <text macro="std-authority-child"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="std-authority-parent">
    <names variable="authority">
      <name/>
      <institution institution-parts="short" use-last="1"/>
    </names>
  </macro>
  <macro name="std-authority-child">
    <names variable="authority">
      <name/>
      <institution institution-parts="short" use-first="1"/>
    </names>
  </macro>
  <macro name="editor-translator">
    <group delimiter=", ">
      <choose>
        <if variable="author">
          <group delimiter=" ">
            <choose>
              <if variable="container-author">
                <group>
                  <names variable="container-author">
                    <label form="verb-short" suffix=" " text-case="lowercase"/>
                    <name and="text" delimiter=", "/>
                  </names>
                </group>
              </if>
            </choose>
          </group>
          <names delimiter=", " variable="editor translator">
            <label form="verb-short" suffix=" " text-case="lowercase"/>
            <name and="text" delimiter=", "/>
          </names>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="secondary-contributors-note">
    <choose>
      <if match="none" type="broadcast chapter paper-conference">
        <text macro="editor-translator"/>
      </if>
      <else-if type="broadcast">
        <text macro="director-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="container-contributors-note">
    <choose>
      <if match="any" type="chapter paper-conference">
        <text macro="editor-translator"/>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="broadcast chapter paper-conference">
        <group delimiter=". ">
          <choose>
            <if variable="author">
              <names delimiter=". " variable="editor translator">
                <label form="verb" suffix=" " text-case="capitalize-first"/>
                <name and="text" delimiter=", "/>
              </names>
            </if>
          </choose>
        </group>
      </if>
      <else-if type="broadcast">
        <text macro="director"/>
      </else-if>
    </choose>
  </macro>
  <macro name="director">
    <names variable="director">
      <label form="verb" suffix=" " text-case="capitalize-first"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="director-note">
    <names variable="director">
      <label form="verb-short" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <group delimiter=", ">
          <choose>
            <if variable="author">
              <choose>
                <if variable="container-author">
                  <names variable="container-author">
                    <label form="verb-short" suffix=" " text-case="lowercase"/>
                    <name and="text" delimiter=", "/>
                  </names>
                </if>
              </choose>
              
              <choose>
                <if match="all" variable="container-author author">
                  <group delimiter=". ">
                    <number variable="page"/>
                    <names delimiter=", " variable="editor translator">
                      <label form="verb" suffix=" "/>
                      <name and="text" delimiter=", "/>
                    </names>
                  </group>
                </if>
                <else>
                  <names delimiter=", " variable="editor translator">
                    <label form="verb" suffix=" " text-case="lowercase"/>
                    <name and="text" delimiter=", "/>
                  </names>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="recipient-note">
    <names delimiter=", " variable="recipient">
      <label form="verb" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="contributors-note">
    <group delimiter=" ">
      <names variable="author">
        <name and="text" delimiter=", " sort-separator=", "/>
        <label form="short" prefix=", "/>
        <substitute>
          <names variable="composer"/>
          <names variable="editor"/>
          <names variable="translator"/>
        </substitute>
      </names>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name and="text" delimiter=", " delimiter-precedes-last="always" name-as-sort-order="first" sort-separator=", "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="translator">
    <names variable="translator">
      <name and="text" delimiter=", " delimiter-precedes-last="always" name-as-sort-order="first" sort-separator=", "/>
      <label form="verb-short" prefix=", "/>
    </names>
  </macro>
  <macro name="recipient">
    <group delimiter=", ">
      <text macro="recipient-note"/>
      <choose>
        <if type="personal_communication">
          <choose>
            <if variable="genre">
              <text text-case="capitalize-first" variable="genre"/>
            </if>
            <else>
              <text term="letter" text-case="capitalize-first"/>
            </else>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors">
    <group delimiter=". ">
      <names variable="author">
        <name and="text" delimiter=", " delimiter-precedes-last="always" name-as-sort-order="first" sort-separator=", "/>
        <substitute>
          <names variable="composer"/>
          <text macro="editor"/>
          <text macro="translator"/>
        </substitute>
      </names>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="recipient-short">
    <names variable="recipient">
      <label form="verb" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", " form="short"/>
    </names>
  </macro>
  <macro name="contributors-short">
    <group delimiter=" ">
      <names variable="author">
        <name and="text" delimiter=", " form="short"/>
        <substitute>
          <names variable="composer"/>
          <names variable="editor"/>
          <names variable="translator"/>
        </substitute>
      </names>
      <text macro="recipient-short"/>
    </group>
  </macro>
  <macro name="contributors-sort">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="always" name-as-sort-order="all" sort-separator=", "/>
      <label form="verb-short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="interviewer-note">
    <names delimiter=", " variable="interviewer">
      <label form="verb" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="interviewer">
    <names delimiter=", " variable="interviewer">
      <label form="verb" suffix=" " text-case="capitalize-first"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="title-note">
    <choose>
      <if match="none" type="personal_communication" variable="title">
        <text variable="genre"/>
      </if>
      <else-if match="any" type="bill legal_case" variable="jurisdiction">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report">
        <text font-style="italic" text-case="title" variable="title"/>
      </else-if>
      <else-if match="any" type="broadcast song">
        <choose>
          <if match="any" variable="container-title collection-title">
            <text quotes="true" variable="title"/>
          </if>
          <else>
            <text font-style="italic" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text quotes="true" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="none" variable="title">
        <choose>
          <if match="none" type="personal_communication">
            <text text-case="capitalize-first" variable="genre"/>
          </if>
        </choose>
      </if>
      <else-if match="any" type="bill legal_case" variable="jurisdiction">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report">
        <text font-style="italic" text-case="title" variable="title"/>
      </else-if>
      <else-if match="any" type="broadcast song">
        <choose>
          <if match="any" variable="container-title collection-title">
            <text quotes="true" variable="title"/>
          </if>
          <else>
            <text font-style="italic" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text quotes="true" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="title-short">
    <choose>
      <if match="none" variable="title">
        <choose>
          <if type="interview">
            <text term="interview" text-case="lowercase"/>
          </if>
          <else-if match="any" type="manuscript speech">
            <text form="short" variable="genre"/>
          </else-if>
          <else-if type="personal_communication">
            <text macro="issued"/>
          </else-if>
        </choose>
      </if>
      <else-if match="any" type="bill legal_case" variable="jurisdiction">
        <text font-style="italic" form="short" variable="title"/>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report">
        <text font-style="italic" form="short" text-case="title" variable="title"/>
      </else-if>
      <else-if match="any" type="broadcast song">
        <choose>
          <if variable="container-title collection-title">
            <text form="short" quotes="true" variable="title"/>
          </if>
          <else>
            <text font-style="italic" form="short" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text form="short" quotes="true" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="description-note">
    <group delimiter=", ">
      <text macro="interviewer-note"/>
      <choose>
        <if match="none" variable="title"/>
        <else-if match="any" type="thesis speech"/>
        <else-if type="graphic broadcast video motion_picture song"/>
        <else>
          <group delimiter=" ">
            <text form="short" variable="medium"/>
            <text form="short" variable="genre"/>
          </group>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="description">
    <group delimiter=", ">
      <group delimiter=". ">
        <text macro="interviewer"/>
        <text text-case="capitalize-first" variable="medium"/>
      </group>
      <choose>
        <if match="none" variable="title"/>
        <else-if match="any" type="thesis speech"/>
        <else-if type="graphic broadcast video motion_picture song"/>
        <else>
          <text text-case="capitalize-first" variable="genre"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="description-multimedia-note">
    <group delimiter=", ">
      <choose>
        <if type="graphic broadcast video song motion_picture">
          <group delimiter=", ">
            <text form="short" variable="medium"/>
            <text form="short" variable="genre"/>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="description-multimedia">
    <group delimiter=", ">
      <choose>
        <if type="graphic broadcast video song motion_picture">
          <group delimiter=", ">
            <text form="short" variable="medium"/>
            <text form="short" variable="genre"/>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="container-title-note">
    <group delimiter=" ">
      <choose>
        <if match="any" type="chapter">
          <text term="in" text-case="lowercase"/>
        </if>
        <else-if type="paper-conference" variable="event container-title"/>
        <else-if type="paper-conference">
          <text term="in" text-case="lowercase"/>
        </else-if>
      </choose>
      <choose>
        <if match="none" type="legal_case">
          <text font-style="italic" variable="container-title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="container-title">
    <group delimiter=" ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <choose>
        <if match="none" type="legal_case">
          <text font-style="italic" variable="container-title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="collection-title">
    <choose>
      <if type="song">
        <choose>
          <if variable="container-title">
            <group delimiter=" ">
              <text variable="collection-title"/>
              <number variable="collection-number"/>
            </group>
          </if>
          <else>
            <group delimiter=" " font-style="italic">
              <text variable="collection-title"/>
              <number variable="collection-number"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <group delimiter=" ">
          <text variable="collection-title"/>
          <number variable="collection-number"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="edition-action">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <number suffix="." variable="edition"/>
      </else>
    </choose>    
  </macro>
  <macro name="edition-note">
    <choose>
      <if match="any" type="bill book graphic legal_case motion_picture report song chapter paper-conference">
        <text macro="edition-action"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if match="any" type="bill book graphic legal_case motion_picture report song chapter paper-conference">
        <text macro="edition-action"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-note-join-with-space">
    <choose>
      <if type="article-journal">
        <text macro="locators-note"/>
      </if>
      <else-if match="all" type="paper-conference" variable="container-title event">
        <text macro="locators-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-note-join-with-comma">
    <choose>
      <if match="none" type="article-journal paper-conference">
        <text macro="locators-note"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-note-article-action">
    <group delimiter=", ">
      <number variable="volume"/>
      <group delimiter=" ">
        <text form="short" term="issue"/>
        <number variable="issue"/>
      </group>
    </group>
  </macro>
  <macro name="locators-note">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event">
        <text macro="locators-note-article-action"/>
      </if>
      <else-if type="article-journal">
        <text macro="locators-note-article-action"/>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=", ">
          <group delimiter=" ">
            <number variable="volume"/>
            <text variable="container-title"/>
            <number variable="page"/>
          </group>
          <number variable="locator"/>
        </group>
      </else-if>
      <else-if match="any" type="bill book graphic legal_case motion_picture report song chapter">
        <group delimiter=", ">
          <group delimiter=" ">
            <choose>
              <if is-numeric="volume">
                <text form="short" term="volume"/>
              </if>
            </choose>
            <number form="numeric" variable="volume"/>
          </group>
          <choose>
            <if match="none" variable="locator">
              <group delimiter=" ">
                <number form="numeric" variable="number-of-volumes"/>
                <text form="short" plural="true" term="volume"/>
              </group>
            </if>
          </choose>
          <text macro="edition-note"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-join-with-space">
    <choose>
      <if type="article-journal">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-join-with-comma">
    <choose>
      <if type="legal_case">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-join-with-period">
    <choose>
      <if match="none" type="legal_case article-journal">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal">
        <group delimiter=", ">
          <number variable="volume"/>
          <group delimiter=" ">
            <text form="short" term="issue"/>
            <number variable="issue"/>
          </group>
        </group>
      </if>
      <else-if type="legal_case">
        <group delimiter=" ">
          <number variable="volume"/>
          <text variable="container-title"/>
          <number variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="bill book graphic legal_case motion_picture report song">
        <group delimiter=". ">
          <group delimiter=" ">
            <text form="short" term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
          <group delimiter=" ">
            <number form="numeric" variable="number-of-volumes"/>
            <text form="short" plural="true" term="volume"/>
          </group>
          <text macro="edition"/>
        </group>
      </else-if>
      <else-if match="any" type="chapter paper-conference">
        <group delimiter=". ">
          <choose>
            <if match="none" variable="page">
              <group delimiter=" ">
                <text form="short" term="volume" text-case="capitalize-first"/>
                <number form="numeric" variable="volume"/>
              </group>
            </if>
          </choose>
          <text macro="edition"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-newspaper">
    <choose>
      <if type="article-newspaper">
        <group delimiter=", ">
          <text macro="edition-action"/>
          <group delimiter=" ">
            <choose>
              <if is-numeric="section">
                <text form="short" term="section"/>
              </if>
            </choose>
            <text variable="section"/>
            <choose>
              <if is-numeric="section" match="none">
                <text form="long" term="section"/>
              </if>
            </choose>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="event">
    <group delimiter=" ">
      <text term="presented at"/>
      <text variable="event"/>
    </group>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="thesis">
        <text form="short" variable="publisher"/>
      </if>
      <else>
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text form="short" variable="publisher"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <choose>
          <if match="any" type="graphic report">
            <date variable="issued">
              <date-part name="month" suffix=" "/>
              <date-part name="day" suffix=", "/>
              <date-part name="year"/>
            </date>
          </if>
          <else-if type="legal_case">
            <group delimiter=" ">
              <date variable="issued">
                <date-part name="year"/>
              </date>
            </group>
          </else-if>
          <else-if match="any" type="bill book graphic legal_case motion_picture report thesis chapter paper-conference">
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </else-if>
          <else>
            <date variable="issued">
              <date-part name="month" suffix=" "/>
              <date-part name="day" suffix=", "/>
              <date-part name="year"/>
            </date>
          </else>
        </choose>
      </if>
      <else-if variable="locator-date">
        <date date-parts="year-month-day" form="text" variable="locator-date"/>
      </else-if>
      <else-if type="classic"/>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>

  <macro name="point-locators-subsequent">
    <choose>
      <if variable="locator">
        <choose>
          <if locator="page" match="none">
            <group delimiter=", ">
              <choose>
                <if match="none" position="subsequent" type="bill">
                  <group delimiter=" ">
                    <text form="short" term="volume" text-case="lowercase"/>
                    <number form="numeric" variable="volume"/>
                  </group>
                </if>
              </choose>
              <choose>
                <if match="any" type="bill gazette legislation">
                  <text label-form="symbol" macro="std-locator-label"/>
                </if>
                <else>
                  <text label-form="short" macro="std-locator-label"/>
                </else>
              </choose>
            </group>
          </if>
          <else-if match="any" type="bill book gazette graphic legislation legal_case motion_picture report song">
            <choose>
              <if match="none" type="bill gazette legislation">
                <group delimiter=":">
                  <number form="numeric" variable="volume"/>
                  <number variable="locator"/>
                </group>
              </if>
              <else>
                <text label-form="symbol" macro="std-locator-label"/>
              </else>
            </choose>
          </else-if>
          <else>
            <text label-form="short" macro="std-locator-non-page-label"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="point-locators-join-with-colon">
