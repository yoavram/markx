  <macro name="cms-secondary-first">
    <group delimiter=", ">
      <group delimiter=": ">
        <group delimiter=", ">
          <group delimiter=" ">
            <group delimiter=", ">
              <group delimiter=" ">
                <group delimiter=", ">
                  <group delimiter=", ">
                    <group delimiter=", ">
                      <text macro="contributors-note"/>
                      <text macro="title-note"/>
                    </group>
                    <text macro="description-note"/>
                  </group>
                  <text macro="secondary-contributors-note"/>
