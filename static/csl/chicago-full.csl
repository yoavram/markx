        <date date-parts="year-month-day" form="text" prefix="(" suffix=")" variable="original-date"/>
      </group>
    </group>
  </macro>
  <macro name="us-bill-middle">
    <group delimiter=", ">
      <choose>
        <if variable="title">
          <text macro="us-bill-name-stub"/>
        </if>
      </choose>
      <group delimiter=" ">
