          <else>
            <text macro="us-report-first"/>
          </else>
        </choose>
      </if>
      <else-if jurisdiction="gb">
        <choose>
          <if position="near-note">
            <text macro="gb-report-subsequent"/>
          </if>
          <else>
            <text macro="gb-report-first"/>
          </else>
        </choose>
      </else-if>
      <else-if jurisdiction="europa.eu">
